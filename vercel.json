{
    "version": 2,
    "builds": [
        {
            "src": "/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/robots.txt",
            "dest": "/robots.txt"
        },
        {
            "src": "/(.*)",
            "dest": "/server.ts",
            "headers": {
                "Content-Security-Policy": "require-trusted-types-for 'script';"
            }
        }
    ]
}